<style lang="css">
   :global(.darkmode .new-bg-gradient)
 {
 background: linear-gradient(315deg, var(--primary-200) 25%, var(--secondary-400)); 
 filter: grayscale(35%);
 backdrop-filter: blur(89px);

 }
 .new-bg-gradient
 {
  background: linear-gradient(95deg, var(--primary-100) 10%, var(--secondary-300));
 }
  .text-gradient {
    background:linear-gradient(135deg,rgb(65,88,208) 0%,rgb(199,81,192) 50%, rgb(255,203,112) 100%) ;
    background-clip: border-box;
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    z-index: 20;
  }

  :global(.darkmode .text-gradient) {
    background:linear-gradient(135deg,rgb(255,203,112) 0%,rgb(199,81,192) 50%,rgb(65,88,208) 100%);
    background-clip: border-box;
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
  }
  .animated-heading {
  animation: animateLetterSpacing 4s ease-in-out;
}

@keyframes animateLetterSpacing {
  0% {
    letter-spacing: 3px; 
    transform: scale(1.2, 5.2);
  }
  50%{
    transform: scale(1.1, 2.1);
    /* letter-spacing: 0px;  */
  }
  100% {
    letter-spacing: 0px;/* Adjust the letter-spacing value as needed */
    transform: scale(1);
  }
}
</style>
